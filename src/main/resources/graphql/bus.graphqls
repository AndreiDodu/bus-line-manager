type Bus {
    id: ID!
    capacity: Int!
    busLineId: ID
}

input BusInsertInput {
    capacity: Int!
    busLineId: ID
}

type Query {
    getBus(id: ID!): Bus
	getBuses: [Bus]
}

type Mutation{
	insertBus(data: BusInsertInput!): Bus!
	updateBus(id: ID!, data: BusInsertInput!): Bus!
	deleteBus(id: ID!): Boolean!
}